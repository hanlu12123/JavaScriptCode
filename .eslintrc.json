{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "App": "readonly",
    "Page": "readonly",
    "Component": "readonly",
    "getCurrentPages": "readonly",
    "Behavior": "readonly",
    "ty": true,
    "I18n": true,
    "getApp": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-extra-boolean-cast": "off",
    "no-unused-vars": "off"
  }
}
